{
	"Version": "2012-10-17",
	"Statement": [
		{
			"Effect": "Allow",
			"Action": [
				"ec2:StartInstances",
				"ec2:StopInstances",
				"ec2:TerminateInstances",
				"ec2:DescribeInstances"
			],
			"Resource": "*"
		},
		{
			"Effect": "Allow",
			"Action": [
				"rds-data:ExecuteStatement",
				"rds-data:BatchExecuteStatement",
				"rds-data:BeginTransaction",
				"rds-data:CommitTransaction",
				"rds-data:RollbackTransaction"
			],
			"Resource": "*"
		}
	]
}